@viewModel DotVVM.WebForms.Tutorial.DotvvmSamples._09_checkboxlist.ViewModels.DefaultViewModel, DotVVM.WebForms.Tutorial

<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>

    <dot:Repeater DataSource="{value: Countries}">
        <dot:CheckBox Text="{value: Name}" CheckedValue="{value: Id}"
                      CheckedItems="{value: _root.SelectedIds}" />
    </dot:Repeater>

</body>
</html>


